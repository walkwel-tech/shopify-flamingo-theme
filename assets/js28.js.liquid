timber.sortFilters = function () {
  timber.cache.$advancedFilters.each(function () {
    var $el = $(this),
        $tags = $el.find('li'),
        aNumber = /\d+/,
        sorted = false;
    $tags.sort(function (a, b) {
      a = parseInt( aNumber.exec( $(a).text() ), 10 );
      b = parseInt( aNumber.exec( $(b).text() ), 10 );
      if ( isNaN(a)  || isNaN(b) ) {
        return;
      }
      else {
        sorted = true;
        return a - b;
      }
    });
    if (sorted) {
      $el.append($tags);
    }
  });
};

timber.formatMoney = function (val) {
  

  

  
    if (ShopScript.vars.moneyFormat.indexOf('money') === -1) {
      if ( (ShopScript.vars.moneyFormat.indexOf('{{amount}}') > -1) && (ShopScript.vars.moneyFormat.indexOf('.') === -1) ) {
        val = val.replace('$','<sup class="dollar">$</sup> ').replace('.','<sup class="cents">') + '</sup>';
      }
      else if (ShopScript.vars.moneyFormat.indexOf('{{amount_with_comma_separator}}') > -1) {
        val = val.replace('$','<sup class="dollar">$</sup> ').replace(',','<sup class="cents">') + '</sup>';
      }
    }
  

  return val;
};

timber.formatSaleTag = function (val) {
  // If not using multiple currencies
  if (ShopScript.vars.moneyFormat.indexOf('money') === -1) {
    // If we use amount
    if ( (ShopScript.vars.moneyFormat.indexOf('{{amount}}') > -1) && (ShopScript.vars.moneyFormat.indexOf('.') === -1) ) {
      // If there are no cents or money amount is more than 10, remove decimals
      if ( (val.indexOf('.00') > -1) || parseInt(val.replace(/[^0-9]/g, ''), 10) > 1000 ) {
        return val.split('.')[0];
      }
    }
    // If we use amount_with_comma_separator
    else if (ShopScript.vars.moneyFormat.indexOf('{{amount_with_comma_separator}}') > -1) {
      // If there are no cents or money amount is more than 10, remove decimals
      if ( (val.indexOf(',00') > -1) || parseInt(val.replace(/[^0-9]/g, ''), 10) > 1000 ) {
        return val.split(',')[0];
      }
    }
  }
  return val;
};



