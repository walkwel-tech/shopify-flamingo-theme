// Smooth scrolling on hashed anchors
function smoothScrollingOnAnchors() {
  $(document).on('click', 'a.pageScrollBtn[href*="#"]:not([href="#"])', function(e) {
    if (location.pathname.replace(/^\//,'') === this.pathname.replace(/^\//,'') && location.hostname === this.hostname) {
      var target = $(this.hash);
      target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
      if (target.length) {
        $('html, body').animate({
          scrollTop: target.offset().top
        }, 1000);
        e.preventDefault();
      }
    }
  });
}



ShopScript.slideoutNav = function() {

  ShopScript.cache.showSlideoutClass = 'slideout-nav--open';
  ShopScript.openSlideoutMenu = function() {
    if ( isMobile() ) {
      ShopScript.cache.$slideoutNav.addClass(ShopScript.cache.showSlideoutClass);
    }
    ShopScript.cache.$body.addClass('body-no-scroll');
  }
  ShopScript.closeSlideoutMenu = function() {
    if ( isMobile() ) {
      ShopScript.cache.$slideoutNav.removeClass(ShopScript.cache.showSlideoutClass);
    }
    ShopScript.cache.$body.removeClass('body-no-scroll');
  }
  ShopScript.populateSlideoutNav = function(triggerElement) {
    if ( !$(triggerElement).hasClass('nav-loaded') ) {
      ShopScript.cache.$slideoutNav.find('.sn--inner').append(ShopScript.cache.$loadingSpinner3x);
      $.ajax({url: "https://dankstop.com/?view=slideoutNav", success: function(slideoutHtml){
        ShopScript.cache.$slideoutNav.addClass('nav-loaded').find('.sn--inner').hide().html(slideoutHtml).fadeIn(753);
        ShopScript.cache.$slideoutNavTrigger.addClass('nav-loaded');
        ShopScript.cache.$slideoutCloseBtn = $('#closeSlideoutBtn');
        ShopScript.cache.$slideoutCloseBtn.on('click', function() {
          ShopScript.closeSlideoutMenu();
        });
      }});
    }
  }
  
  if ( isMobile() ) {
    ShopScript.cache.$slideoutNavTrigger.on('click', function() {
      if ( ShopScript.cache.$slideoutNav.hasClass(ShopScript.cache.showSlideoutClass) ) {
        ShopScript.closeSlideoutMenu();
      }
      else {
        ShopScript.openSlideoutMenu();
      }
      ShopScript.populateSlideoutNav(ShopScript.cache.$slideoutNavTrigger);
    });
  }
  else {
    ShopScript.cache.$slideoutNav.on({
      mouseenter: function(event) {
        ShopScript.cache.$body.addClass('modal-open modal-full');
        ShopScript.populateSlideoutNav(ShopScript.cache.$slideoutNav);
      },
      mouseleave: function(event) {
        ShopScript.cache.$body.removeClass('modal-open modal-full');
      }
    });
  }

};



