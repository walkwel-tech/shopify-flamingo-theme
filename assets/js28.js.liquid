ShopScript.wishList = function() {
  var wishlistUrl = '//api.dankstop.com/ds_public_api/wishlist.php';
  var customerId = $('#customerAccountLink').data('customerid') ? $('#customerAccountLink').data('customerid') : 'NULL';
  var usersWishlist = '';
  var addToWLText = 'Add To Wishlist';
  var removeFromWLText = 'Remove From Wishlist';
  var $addToWishlistBtn = $('.wl-add');

  $addToWishlistBtn.each(function(){
    attachWishlistHandler($(this));
  });
  function attachWishlistHandler( wishlistBtn ) {
    wishlistBtn.one('click', function() {
      var $this = $(this);
      var productId = $this.data('product');
      var $wLText = $this.find('.wishlist-title');
      $this.addClass('loading-spinner');
      var wishlistAction = 'addToWishlist';
      if ($this.hasClass('in-wishlist')) {
        $this.removeClass('in-wishlist');
        $wLText.text(addToWLText);
        wishlistAction = 'removeFromWishlist';
      }
      else {
        $this.addClass('in-wishlist');
        $wLText.text(removeFromWLText);
      }
      var wishlistClickParams = {'wishlistAction': wishlistAction, 
                                 'customerId': customerId,
                                 'productHandle': productId};
      $.ajax({
        url: wishlistUrl,
        type: "POST",
        dataType:'json',
        data: wishlistClickParams,
        success: function(updateWishlistResponse){
          usersWishlist = updateWishlistResponse;
          attachWishlistHandler($this);
          $this.removeClass('loading-spinner');
        }
      });
    });
  }
}






