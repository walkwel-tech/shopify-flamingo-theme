ShopScript.init = function () {

  var lazyCssSrcs = [
    'https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css'
  ];
  var jQuerySrc = ['//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js'];

  var jsSrcs = [];

  ShopScript.loadStyleSheets(lazyCssSrcs);
  checkForJQuery();
  
  function downloadDependenciesOnLoad(jsSrcs) {
   // if (window.addEventListener)
      //window.addEventListener("load", ShopScript.downloadJSScript(jsSrcs), false);
    //else if (window.attachEvent)
      //window.attachEvent("onload", ShopScript.downloadJSScript(jsSrcs));
   // else window.onload = ShopScript.downloadJSScript(jsSrcs);
    //ShopScript.controlPanel();
  }

  function checkForJQuery() {
    var msBeforeRecheck = 137;
    var jQueryCheckCounter = 0;
    var jQueryCheckLimit = Math.ceil(1000/msBeforeRecheck);
    runJQCheck();
    function runJQCheck() {
      if ( typeof jQuery !== 'undefined' && window.jQuery ) {
        downloadDependenciesOnLoad(jsSrcs);
      } else {
        if (jQueryCheckCounter < jQueryCheckLimit) {
          setTimeout(function(){
            jQueryCheckCounter++;
            runJQCheck();
          }, 137);
        }
        else {
          ShopScript.downloadJSScript(jQuerySrc);
          downloadDependenciesOnLoad(jsSrcs);
        }
      }
    }
  }
  
};



ShopScript.ytVideos = function() {
  var ytVid = document.querySelectorAll( ".yt-video" );
  for (var i = 0; i < ytVid.length; i++) {
    var source = "https://img.youtube.com/vi/"+ ytVid[i].dataset.embed +"/0.jpg"; 
    var image = new Image();
    image.src = source;
    image.addEventListener( "load", function() {
      ytVid[ i ].appendChild( image );
    }( i ) );
    ytVid[i].addEventListener( "click", function() {
      var iframe = document.createElement( "iframe" );
      iframe.setAttribute( "frameborder", "0" );
      iframe.setAttribute( "allowfullscreen", "" );
      iframe.setAttribute( "src", "https://www.youtube.com/embed/"+ this.dataset.embed +"?wmode=transparent&enablejsapi=1&modestbranding=1&showinfo=0&controls=0&rel=0&loop=1&theme=light&fs=0&origin=" + window.location.hostname );

      this.innerHTML = "";
      this.appendChild( iframe );
    } );
  }
};



ShopScript.soshLoginOnDemand = function() {
  var $mobileAccountTrigger = $('#mnAccTrgr');
  var $mobileMenuSocialContainer = $('.mobile-nav .social-login-cont');
  var $loginRegPageContainer = $('.main-content .social-login-cont');
  function loadSocialLogin($appendToThis) {
    var oxiIframe = document.createElement('iframe');
    oxiIframe.id = "social_login_frame";
    oxiIframe.src = "https://social-login.oxiapps.com/widget?site=dankstop-8.myshopify.com";
    oxiIframe.style.width = oxiIframe.style.maxWidth = "100%";
    oxiIframe.style.height = "240px";
    oxiIframe.style.border = "0";
    oxiIframe.style.paddingTop = "0";
    oxiIframe.style.paddingBottom = "5px";
    oxiIframe.scrolling = "no";
    $appendToThis.each(function(){
      $(this).append($(oxiIframe).clone());
    });
    var response = function(event) {
      if (event.origin+'/'=="https://social-login.oxiapps.com/") {
        var message = event.data;
        var key = message.split(':')[0];
        var value = message.split(':')[1];
        if (key=="height") {
          var oxi_iframe = document.getElementById('social_login_frame');
          if (oxi_iframe) {
            oxi_iframe.style.height = value + "px";
          }
        }
      }
    }
    if (window.addEventListener) {
      window.addEventListener('message', response, false);
    } else {
      window.attachEvent("onmessage", response);
    }
  }
  $mobileAccountTrigger.on('change', function(){
    if ( $mobileMenuSocialContainer.first().find('iframe').length === 0 ) {
      loadSocialLogin($mobileMenuSocialContainer);
    }
  });
  if ( ShopScript.vars.originalPathname === '/account/login' || ShopScript.vars.originalPathname === '/account/register' ) {
    loadSocialLogin($loginRegPageContainer);
  }
}



