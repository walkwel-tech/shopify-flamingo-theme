        revertImg = setTimeout( function() {
          $thisPGIImage.attr({
            'src': originalImage,
            'srcset': originalSrcset
          });
        }, 370);
      }
    }
    else {
      clearTimeout(revertImg);
      if ($thisPGI.find('.vd-btn.videoPlaying').length > 0) {
