ShopScript.downloadJSScript = function(jsSrcToLazyload) {
  var i;
  var jsSrcs = typeof jsSrcToLazyload === 'string' ? [jsSrcToLazyload] : jsSrcToLazyload;
  for (i = 0; i < jsSrcs.length; i++) {
    var deferredScript = document.createElement("script");
    deferredScript.src = jsSrcs[i];
    document.getElementsByTagName('body')[0].appendChild(deferredScript);
  }    
}


ShopScript.loadStyleSheets = function(cssSrcsToLazyLoad) {
  var cssSrcs = typeof cssSrcsToLazyLoad === 'string' ? [cssSrcsToLazyLoad] : cssSrcsToLazyLoad;
  if (document.createStyleSheet){
    var i;
    for (i = 0; i < cssSrcs.length; i++) {
      document.createStyleSheet(cssSrcs[i]);
    }
  }
  else {
    var i;
    for (i = 0; i < cssSrcs.length; i++) {
      var deferredStylesheet = document.createElement("link");
      deferredStylesheet.rel = 'stylesheet';
      deferredStylesheet.type = 'text/css';
      deferredStylesheet.media = 'screen';
      deferredStylesheet.href = cssSrcs[i];
      document.getElementsByTagName('head')[0].appendChild(deferredStylesheet);
    }
  }
};


